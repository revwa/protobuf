{
  "name": "protobuf",
  "version": "1.0.0",
  "description": "Automatically generated Protobuf functions for WhatsApp",
  "scripts": {
    "fetch": "node build/fetch.js",
    "parse": "node build/parse.js",
    "build:proto": "tsc --project tsconfig.proto.json",
    "build:parser": "tsc --project tsconfig.parser.json",
    "dev:fetch": "ts-node src/fetch.ts",
    "dev:parse": "ts-node src/parse.ts",
    "dev:all": "npm run fetch && npm run parse"
  },
  "keywords": ["protobuf", "whatsapp", "typescript", "proto", "proto2"],
  "author": "ponei",
  "license": "MIT",
  "devDependencies": {
    "@babel/types": "^7.21.2",
    "@types/node": "^18.14.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@babel/parser": "^7.21.2",
    "axios": "^1.3.3",
    "long": "^5.2.1",
    "protobufjs": "^7.2.2",
    "ts-proto": "^1.139.0"
  }
}
